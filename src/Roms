# Rules to build the image files
bin32/3c595.tmp:	bin32/3c595.o bin32/config-3c595.o bin32/pci.o $(STDDEPS32)
	$(LD32) $(LDFLAGS32) -o $@ $(START32) bin32/config-3c595.o bin32/3c595.o bin32/pci.o $(LIBS32)
	@$(SIZE32) $@ | $(CHECKSIZE)

bin32/3c595--%.tmp:	bin32/3c595.o bin32/%.o bin32/config-3c595--%.o bin32/pci.o $(STDDEPS32)
	$(LD32) $(LDFLAGS32) -o $@ $(START32) bin32/config-3c595--$*.o bin32/3c595.o bin32/$*.o bin32/pci.o $(LIBS32)
	@$(SIZE32) $@ | $(CHECKSIZE)

bin32/3c595.elf:	bin32/3c595.o bin32/config-3c595.o bin32/pci.o $(ELF_DEPS32)
	$(LD32) $(LDFLAGS32) -o $@ $(ELF_START32) bin32/config-3c595.o bin32/3c595.o bin32/pci.o $(LIBS32)
	@$(SIZE32) $@ | $(CHECKSIZE)

bin32/3c595--%.elf:	bin32/3c595.o bin32/%.o bin32/config-3c595--%.o bin32/pci.o $(ELF_DEPS32)
	$(LD32) $(LDFLAGS32) -o $@ $(ELF_START32) bin32/config-3c595--$*.o bin32/3c595.o bin32/$*.o bin32/pci.o $(LIBS32)
	@$(SIZE32) $@ | $(CHECKSIZE)

bin32/3c595.img:	bin32/3c595.o bin32/3c595.tmp bin32/config-3c595.o bin32/pci.o $(STDDEPS32)
	$(LD32) $(LDFLAGS32) $(LDBINARY32) -o $@ $(START32) bin32/config-3c595.o bin32/3c595.o bin32/pci.o $(LIBS32)

bin32/3c595--%.img:	bin32/3c595.o bin32/%.o bin32/3c595.tmp bin32/config-3c595--%.o bin32/pci.o $(STDDEPS32)
	$(LD32) $(LDFLAGS32) $(LDBINARY32) -o $@ $(START32) bin32/config-3c595--$*.o bin32/3c595.o bin32/$*.o bin32/pci.o $(LIBS32)

bin32/3c90x.tmp:	bin32/3c90x.o bin32/config-3c90x.o bin32/pci.o $(STDDEPS32)
	$(LD32) $(LDFLAGS32) -o $@ $(START32) bin32/config-3c90x.o bin32/3c90x.o bin32/pci.o $(LIBS32)
	@$(SIZE32) $@ | $(CHECKSIZE)

bin32/3c90x--%.tmp:	bin32/3c90x.o bin32/%.o bin32/config-3c90x--%.o bin32/pci.o $(STDDEPS32)
	$(LD32) $(LDFLAGS32) -o $@ $(START32) bin32/config-3c90x--$*.o bin32/3c90x.o bin32/$*.o bin32/pci.o $(LIBS32)
	@$(SIZE32) $@ | $(CHECKSIZE)

bin32/3c90x.elf:	bin32/3c90x.o bin32/config-3c90x.o bin32/pci.o $(ELF_DEPS32)
	$(LD32) $(LDFLAGS32) -o $@ $(ELF_START32) bin32/config-3c90x.o bin32/3c90x.o bin32/pci.o $(LIBS32)
	@$(SIZE32) $@ | $(CHECKSIZE)

bin32/3c90x--%.elf:	bin32/3c90x.o bin32/%.o bin32/config-3c90x--%.o bin32/pci.o $(ELF_DEPS32)
	$(LD32) $(LDFLAGS32) -o $@ $(ELF_START32) bin32/config-3c90x--$*.o bin32/3c90x.o bin32/$*.o bin32/pci.o $(LIBS32)
	@$(SIZE32) $@ | $(CHECKSIZE)

bin32/3c90x.img:	bin32/3c90x.o bin32/3c90x.tmp bin32/config-3c90x.o bin32/pci.o $(STDDEPS32)
	$(LD32) $(LDFLAGS32) $(LDBINARY32) -o $@ $(START32) bin32/config-3c90x.o bin32/3c90x.o bin32/pci.o $(LIBS32)

bin32/3c90x--%.img:	bin32/3c90x.o bin32/%.o bin32/3c90x.tmp bin32/config-3c90x--%.o bin32/pci.o $(STDDEPS32)
