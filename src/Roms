bin16/3c503.o:	ns8390.c $(MAKEDEPS) cards.h etherboot.h linux-asm-io.h linux-asm-string.h nic.h ns8390.h osdep.h
	$(CC16) $(CFLAGS16) $(3C503FLAGS) -o $@ -c $<

bin16/config-3c503.o:	config.c $(MAKEDEPS) osdep.h etherboot.h nic.h cards.h
	$(CC16) $(CFLAGS16) -DINCLUDE_3C503 -o $@ -c $<

