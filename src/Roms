# Rules to build the ROM files
bin32/3c590.rom:	bin32/3c595.img $(PRLOADER)
	cat $(PRLOADER) $< > $@
	bin/makerom $(MAKEROM_$*) -p 0x10b7,0x5900 -i$(IDENT32) $@

bin32/3c590.lzrom:	bin32/3c595.huf $(PRZLOADER)
	cat $(PRZLOADER) $< > $@
	bin/makerom $(MAKEROM_$*) -p 0x10b7,0x5900 -i$(IDENT32) $@

