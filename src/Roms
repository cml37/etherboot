# Rules to build the driver object files
INCLUDE_3c595 = -DINCLUDE_3C595

bin32/3c595.o:	3c595.c $(MAKEDEPS) pci.h 3c595.h etherboot.h linux-asm-io.h linux-asm-string.h nic.h osdep.h timer.h
	$(CC32) $(CFLAGS32) $(3C595FLAGS) -o $@ -c $<

bin32/config-3c595.o:	config.c $(MAKEDEPS) osdep.h etherboot.h nic.h cards.h
	$(CC32) $(CFLAGS32) $(INCLUDE_3c595) -o $@ -c $<

bin32/config-3c595--%.o:	config.c $(MAKEDEPS) osdep.h etherboot.h nic.h cards.h
	$(CC32) $(CFLAGS32) $(INCLUDE_3c595) $(INCLUDE_$*) -DTRY_ALL_DEVICES -o $@ -c $<

INCLUDE_3c90x = -DINCLUDE_3C90X

bin32/3c90x.o:	3c90x.c $(MAKEDEPS) pci.h cards.h etherboot.h linux-asm-io.h linux-asm-string.h nic.h osdep.h timer.h
	$(CC32) $(CFLAGS32) $(3C90XFLAGS) -o $@ -c $<

bin32/config-3c90x.o:	config.c $(MAKEDEPS) osdep.h etherboot.h nic.h cards.h
	$(CC32) $(CFLAGS32) $(INCLUDE_3c90x) -o $@ -c $<

bin32/config-3c90x--%.o:	config.c $(MAKEDEPS) osdep.h etherboot.h nic.h cards.h
	$(CC32) $(CFLAGS32) $(INCLUDE_3c90x) $(INCLUDE_$*) -DTRY_ALL_DEVICES -o $@ -c $<

