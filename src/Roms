# Rules to build the driver object files

bin32/3c595.o:	3c595.c $(MAKEDEPS) pci.h 3c595.h dev.h etherboot.h isa.h linux-asm-io.h linux-asm-string.h nic.h osdep.h stdint.h timer.h
	$(CC32) $(CFLAGS32) $(3C595FLAGS) -o $@ -c $<

bin32/3c595--%.o:	bin32/%.o bin32/3c595.o $(MAKEDEPS)
	$(LD32) -r bin32/3c595.o $< -o $@


bin32/3c90x.o:	3c90x.c $(MAKEDEPS) pci.h dev.h etherboot.h isa.h linux-asm-io.h linux-asm-string.h nic.h osdep.h stdint.h timer.h
	$(CC32) $(CFLAGS32) $(3C90XFLAGS) -o $@ -c $<

bin32/3c90x--%.o:	bin32/%.o bin32/3c90x.o $(MAKEDEPS)
	$(LD32) -r bin32/3c90x.o $< -o $@


bin32/davicom.o:	davicom.c $(MAKEDEPS) pci.h dev.h etherboot.h isa.h linux-asm-io.h linux-asm-string.h nic.h osdep.h stdint.h
	$(CC32) $(CFLAGS32) $(DAVICOMFLAGS) -o $@ -c $<

bin32/davicom--%.o:	bin32/%.o bin32/davicom.o $(MAKEDEPS)
	$(LD32) -r bin32/davicom.o $< -o $@


bin32/e1000.o:	e1000.c $(MAKEDEPS) pci.h dev.h e1000_hw.h etherboot.h isa.h linux-asm-io.h linux-asm-string.h nic.h osdep.h stdint.h timer.h
	$(CC32) $(CFLAGS32) $(E1000FLAGS) -o $@ -c $<

bin32/e1000--%.o:	bin32/%.o bin32/e1000.o $(MAKEDEPS)
	$(LD32) -r bin32/e1000.o $< -o $@


bin32/eepro100.o:	eepro100.c $(MAKEDEPS) pci.h dev.h etherboot.h isa.h linux-asm-io.h linux-asm-string.h nic.h osdep.h stdint.h timer.h
	$(CC32) $(CFLAGS32) $(EEPRO100FLAGS) -o $@ -c $<

bin32/eepro100--%.o:	bin32/%.o bin32/eepro100.o $(MAKEDEPS)
	$(LD32) -r bin32/eepro100.o $< -o $@


bin32/epic100.o:	epic100.c $(MAKEDEPS) pci.h dev.h epic100.h etherboot.h isa.h linux-asm-io.h linux-asm-string.h nic.h osdep.h stdint.h timer.h
	$(CC32) $(CFLAGS32) $(EPIC100FLAGS) -o $@ -c $<

bin32/epic100--%.o:	bin32/%.o bin32/epic100.o $(MAKEDEPS)
	$(LD32) -r bin32/epic100.o $< -o $@


bin32/fa311.o:	fa311.c $(MAKEDEPS) pci.h dev.h etherboot.h isa.h linux-asm-io.h linux-asm-string.h nic.h osdep.h stdint.h
	$(CC32) $(CFLAGS32) $(FA311FLAGS) -o $@ -c $<

