# Rules to build the ROM files
bin32/3c590.rom:	bin32/3c595.img $(PRLOADER) $(START16)
	cat $(PRLOADER) $(START16) $< > $@
	$(MAKEROM) $(MAKEROM_FLAGS) $(MAKEROM_$*) -p 0x10b7,0x5900 -i$(IDENT32) $@

bin32/3c590--%.rom:	bin32/3c595--%.img $(PRLOADER) $(START16)
	cat $(PRLOADER) $(START16) $< > $@
	$(MAKEROM) $(MAKEROM_FLAGS) $(MAKEROM_$*) -p 0x10b7,0x5900 -i$(IDENT32) $@

bin32/3c590.lzrom:	bin32/3c595.lzimg $(PRLOADER) $(START16)
	cat $(PRLOADER) $(START16) $< > $@
	$(MAKEROM) $(MAKEROM_FLAGS) $(MAKEROM_$*) -p 0x10b7,0x5900 -i$(IDENT32) $@

bin32/3c590--%.lzrom:	bin32/3c595--%.huf $(PRLOADER) $(START16)
	cat $(PRLOADER) $(START16) $< > $@
	$(MAKEROM) $(MAKEROM_FLAGS) $(MAKEROM_$*) -p 0x10b7,0x5900 -i$(IDENT32) $@


bin32/3c590.nrv2brom:	bin32/3c595.nrv2bimg $(PRLOADER) $(START16)
	cat $(PRLOADER) $(START16) $< > $@
	$(MAKEROM) $(MAKEROM_FLAGS) $(MAKEROM_$*) -p 0x10b7,0x5900 -i$(IDENT32) $@

bin32/3c590--%.nrv2brom:	bin32/3c595--%.nrv2bimg $(PRLOADER) $(START16)
	cat $(PRLOADER) $(START16) $< > $@
	$(MAKEROM) $(MAKEROM_FLAGS) $(MAKEROM_$*) -p 0x10b7,0x5900 -i$(IDENT32) $@

