# Rules to build the ROM files
bin32/3c590.rom:	bin32/3c595.img $(PRLOADER)
	cat $(PRLOADER) $< > $@
	$(MAKEROM) $(MAKEROM_$*) -p 0x10b7,0x5900 -i$(IDENT32) $@

bin32/3c590--%.rom:	bin32/3c595--%.img $(PRLOADER)
	cat $(PRLOADER) $< > $@
	$(MAKEROM) $(MAKEROM_$*) -p 0x10b7,0x5900 -i$(IDENT32) $@

bin32/3c590.lzrom:	bin32/3c595.huf $(PRZLOADER)
	cat $(PRZLOADER) $< > $@
	$(MAKEROM) $(MAKEROM_$*) -p 0x10b7,0x5900 -i$(IDENT32) $@

bin32/3c590--%.lzrom:	bin32/3c595--%.huf $(PRZLOADER)
	cat $(PRZLOADER) $< > $@
	$(MAKEROM) $(MAKEROM_$*) -p 0x10b7,0x5900 -i$(IDENT32) $@

bin32/3c590.pxe:	bin32/3c595.img $(PXELOADER)
	cat $(PXELOADER) $< > $@
	$(MAKEROM) -x $(MAKEROM_$*) -p 0x10b7,0x5900 -i$(IDENT32) $@

bin32/3c590--%.pxe:	bin32/3c595--%.img $(PXELOADER)
	cat $(PXELOADER) $< > $@
	$(MAKEROM) -x $(MAKEROM_$*) -p 0x10b7,0x5900 -i$(IDENT32) $@

bin32/3c590.lzpxe:	bin32/3c595.huf $(PXEZLOADER)
	cat $(PXEZLOADER) $< > $@
	$(MAKEROM) -x $(MAKEROM_$*) -p 0x10b7,0x5900 -i$(IDENT32) $@

bin32/3c590--%.lzpxe:	bin32/3c595--%.huf $(PXEZLOADER)
	cat $(PXEZLOADER) $< > $@
	$(MAKEROM) -x $(MAKEROM_$*) -p 0x10b7,0x5900 -i$(IDENT32) $@

bin32/3c590.ebi:	bin32/3c595.elf
	cp bin32/3c595.elf $@
	$(STRIP) -R .comment -R .note $@

bin32/3c595.rom:	bin32/3c595.img $(PRLOADER)
	cat $(PRLOADER) $< > $@
	$(MAKEROM) $(MAKEROM_$*) -p 0x10b7,0x5950 -i$(IDENT32) $@

bin32/3c595--%.rom:	bin32/3c595--%.img $(PRLOADER)
	cat $(PRLOADER) $< > $@
	$(MAKEROM) $(MAKEROM_$*) -p 0x10b7,0x5950 -i$(IDENT32) $@

